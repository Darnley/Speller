<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>3.0.0</Version>
    <Authors>Darnley Costa</Authors>
    <Product>Speller</Product>
    <Description>The symmetric spelling correction algorithm in .NET Core.</Description>
    <PackageLicenseUrl>https://darnley.mit-license.org/2019</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Darnley/Speller</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Darnley/Speller</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Holbor.Speller</PackageId>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="SymSpell" Version="6.3.0" />
  </ItemGroup>

</Project>
